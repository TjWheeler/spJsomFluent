{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./",
    "module": "es6",
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "strictNullChecks": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "moduleResolution": "Node",
    "types": [ "jquery", "sharepoint", "spjsomfluent" ],
    "allowJs": false
  },
  "exclude": [
      "./node_modules"
  ]
}
  
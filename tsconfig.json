{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "target": "es5",
    "sourceMap": true,
    "noImplicitAny": false,
    "noEmitOnError": true,
    "strictNullChecks": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "typeRoots": [ "./typings/@types" ],
    "types": [ "jquery", "sharepoint" ],
    "allowJs": true
  }
}
  